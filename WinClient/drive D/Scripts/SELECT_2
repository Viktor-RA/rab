/****** Скрипт для команды SelectTopNRows из среды SSMS  ******/
DECLARE @CNT INT;

SELECT @CNT = COUNT (*)
 FROM [Parsec4Trans].[dbo].[TRANSLOG]
 WHERE COMP_ID='E7E3AEDA-6093-44A8-8A1B-0B0B0033A84C'

 SELECT *
 FROM [Parsec4Trans].[dbo].[TRANSLOG]
 WHERE COMP_ID='E7E3AEDA-6093-44A8-8A1B-0B0B0033A84C'
 ORDER  BY TRAN_DATE
 OFFSET @CNT - 1 ROWS