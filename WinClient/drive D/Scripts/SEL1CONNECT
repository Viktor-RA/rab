DECLARE @CNT INT;

SELECT @CNT = COUNT (*)
 FROM [Parsec4Trans].[dbo].[TRANSLOG]
 WHERE COMP_ID='9B884D40-BD58-460B-9B09-27604C200746' AND (TRANTYPE_ID='65540' OR TRANTYPE_ID='65667')

 SELECT *
 FROM [Parsec4Trans].[dbo].[TRANSLOG]
 WHERE COMP_ID='9B884D40-BD58-460B-9B09-27604C200746' AND (TRANTYPE_ID='65540' OR TRANTYPE_ID='65667')
 ORDER  BY TRAN_DATE
 OFFSET @CNT - 1 ROWS